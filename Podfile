  pod 'Material'
  pod 'ReSwift'

  target 'MyDiaryiOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MyDiaryiOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
